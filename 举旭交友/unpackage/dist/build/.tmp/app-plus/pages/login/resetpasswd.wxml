<view><cu-custom vue-id="1" hasBorder="{{true}}" isBack="{{true}}" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">重置密码</view></cu-custom><form><view class="cu-form-group margin-top"><view class="title">手机号</view><input placeholder="请输入手机号码" name="pwd1" data-event-opts="{{[['input',[['__set_model',['','phoneNumber','$event',[]]]]]]}}" value="{{phoneNumber}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">验证码</view><input placeholder="请输入验证码" name="pwd2" data-event-opts="{{[['input',[['__set_model',['','code','$event',[]]]]]]}}" value="{{code}}" bindinput="__e"/><button data-event-opts="{{[['tap',[['getCode']]]]}}" class="cu-btn bg-opacity round text-sm" catchtap="__e">{{getCodeText}}</button></view><view class="cu-form-group margin-top"><view class="title">设置密码</view><input type="password" placeholder="请设置不少于8位数字母加数字密码" name="pwd1" data-event-opts="{{[['input',[['__set_model',['','passwd','$event',[]]]]]]}}" value="{{passwd}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">再次确认</view><input type="password" placeholder="请再次确认密码" name="pwd2" data-event-opts="{{[['input',[['__set_model',['','passwd1','$event',[]]]]]]}}" value="{{passwd1}}" bindinput="__e"/></view><view class="padding margin"><button data-event-opts="{{[['tap',[['doReg',['$event']]]]]}}" class="cu-btn block lg bg-green margin" bindtap="__e">确定</button></view></form></view>