<view><cu-custom vue-id="1" hasBorder="{{true}}" isBack="{{true}}" bgColor="nav-bg-gray" bind:__l="__l" vue-slots="{{['content','right']}}"><view slot="content">我的资料</view><view slot="right"><text data-event-opts="{{[['tap',[['handleSave',['$event']]]]]}}" class="margin-right" bindtap="__e">保存</text></view></cu-custom><view class="photo-cont"><view class="photo"><image src="{{baseurl+photo}}" mode="aspectFill"></image><view data-event-opts="{{[['tap',[['ChoosePhoto',['$event']]]]]}}" class="btn" bindtap="__e">上传头像</view></view></view><form><view class="cu-form-group padding-top"><view class="grid col-4 grid-square flex-sub"><block wx:for="{{imgList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="bg-img" data-url="{{imgList[index]}}" data-event-opts="{{[['tap',[['ViewImage',['$event']]]]]}}" bindtap="__e"><image src="{{imgList[index]}}" mode="aspectFill"></image><view class="cu-tag bg-red" data-index="{{index}}" data-event-opts="{{[['tap',[['DelImg',['$event']]]]]}}" catchtap="__e"><text class="cuIcon-close"></text></view></view></block><block wx:if="{{imgList.length<8}}"><view data-event-opts="{{[['tap',[['ChooseImage',['$event']]]]]}}" class="solids" bindtap="__e"><text class="cuIcon-add"></text></view></block></view></view><view class="cu-list menu has-bl"><view class="cu-item">基本资料</view></view><view class="cu-form-group"><view class="title">昵称</view><input class="text-right" placeholder="请输入昵称" name="input" data-event-opts="{{[['input',[['__set_model',['','nickname','$event',[]]]]]]}}" value="{{nickname}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">实名认证</view><block wx:if="{{idcard}}"><button class="cu-btn block line-blue sm">已认证</button></block><block wx:if="{{!idcard}}"><button class="cu-btn block line-blue sm">未认证</button></block></view><block wx:if="{{idcard}}"><view class="cu-form-group"><view class="title">真实姓名</view><input class="text-right" disabled="disabled" value="{{realname}}"/></view></block><block