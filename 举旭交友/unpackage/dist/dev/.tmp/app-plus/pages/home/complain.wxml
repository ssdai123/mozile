<view><cu-custom vue-id="1" hasBorder="{{true}}" isBack="{{true}}" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">投诉</view></cu-custom><form><view class="cu-bar bg-e5e5 solid-bottom"><view class="action color-888 fs-24">请选择投诉类型</view></view><view class="complain-type"><block wx:for="{{complainText}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['changeType',[index,'$0'],[[['complainText','',index,'id']]]]]]]}}" class="{{['item',index==complainIndex?'active':'']}}" bindtap="__e">{{''+item.name+''}}</view></block></view><view class="cu-bar bg-e5e5 solid-bottom"><view class="action color-888 fs-24">请填写投诉内容和上传相关图片证据</view></view><view class="cu-form-group"><textarea placeholder="请留下您宝贵的意见~" data-event-opts="{{[['input',[['__set_model',['','textareaValue','$event',[]]]]]]}}" value="{{textareaValue}}" bindinput="__e"></textarea></view><view class="cu-form-group"><view class="grid col-4 grid-square flex-sub"><block wx:for="{{imgList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="bg-img" data-url="{{imgList[index]}}" data-event-opts="{{[['tap',[['ViewImage',['$event']]]]]}}" bindtap="__e"><image src="{{baseurl+imgList[index]}}" mode="aspectFill"></image><view class="cu-tag bg-red" data-index="{{index}}" data-event-opts="{{[['tap',[['DelImg',['$event']]]]]}}" catchtap="__e"><text class="cuIcon-close"></text></view></view></block><block wx:if="{{imgList.length<4}}"><view data-event-opts="{{[['tap',[['ChooseImage',['$event']]]]]}}" class="solids" bindtap="__e"><text class="cuIcon-camera"></text></view></block></view></view><view class="padding"><button data-event-opts="{{[['tap',[['handleSub',['$event']]]]]}}" class="u-btn block lg bg-green margin" bindtap="__e">提交</button></view></form><block wx:if="{{isShow}}"><view data-event-opts="{{[['tap',[['coverHide',['$event']]]]]}}" class="cover" bindtap="__e"><view class="cover-cont padding flex flex-direction justify-center"><view class="icon-cont bg-green margin-bottom"><image src="../../../static/img/icon/icon_feed.png" mode></image></view><view class="padding-sm"></view><view class="padding margin text-center">谢谢您的反馈，我们将第一时间联系您～</view><view class="text-center"><button data-event-opts="{{[['tap',[['backHome',['$event']]]]]}}" class="cu-btn round lg bg-green magin" catchtap="__e">返回首页</button></view></view></view></block></view>